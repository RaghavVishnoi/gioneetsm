  <div id="content-wrapper">
        
          <div class="row">
            <div class="col-lg-12">
            <div class="main-box clearfix">
            <div class="clearfix">
              <div class="common_page gsb1">
              <div class="hdr">
                   <div class="row">
                <div class="col-md-4">
                  <h4>Gionee Good Morning </h4>
                </div>
                 <div class="col-md-5">
                </div>
                <div class="col-md-3 pull-right">
                  <%=render 'search'%>
                </div> 
              </div>    
                     </div>
             
              <%@targets.each do |target|%>   
              
                 <div id='content' class="tab-content">
                    <div class="tab-pane active" id="all">
                      <div class="row">
                      <div class="col-md-5">
                        <p><b>MUM:</b> <span class="blue"> <%=target.mum%> </span> </p>
                       <p><b>RDS:</b> <span class="blue"> <%=target.rds%> </span>  </p>
                       <p><b>FOS:</b> <span class="blue"> <%=target.fos%> </span>  </p>
                       <p><b>Location Code: </b><span class="blue"><%=target.location_code%></span></p>
                      </div>

                      <div class="col-md-5">
                        <p><b>Date:</b> <span class="blue"> <%=target.date%> </span> </p>
                       <p><b>Value Target:</b> <span class="blue"> <%=target.value_target%> </span>  </p>
                       <p><b>Quantity Target:</b> <span class="blue"> <%=target.volume_target%> </span>  </p>
                       <p><b>IMEI: </b><span class="blue"><%=target.imei%></span></p>
                      </div>

                      <div class="col-md-2">
                        <ul class="list-inline list-unstyled">
                        <li class="blue"><i class="fa fa-hand-o-right"></i><span><%=link_to :show,target%></span></li>
                          
                        </ul>
                      </div>
                    </div>
                   </div>
               </div>
               
              <%end%>
              <div data-no-turbolink>
                <%= will_paginate @retailers %>
              </div>  
                </div>
              </div>
              </div>
            </div>
          </div>
          
          <footer id="footer-bar" class="row">
            <p id="footer-copyright" class="col-xs-12">
              Powered by Gionee
            </p>
          </footer>
        </div>
      </div>
    </div>
  </div>
  <script>
    $("#target").attr("class","active")
  </script>  